{
  "name": "simple-adventure-server",
  "version": "1.0.0",
  "description": "Simple MMO RPG pixel game based on websocket.",
  "main": "index.js",
  "scripts": {
    "serve": "docker-compose -f docker-compose-socket-io.yml up",
    "socket-io:serve": "SERVER_MODE=socket-io ts-node-dev --respawn src/index.ts",
    "socket-io:serve2": "SERVER_MODE=socket-io nodemon --exec node --loader ts-node/esm --respawn src/index.ts",
    "sock-js:serve": "SERVER_MODE=sock-js ts-node-dev --respawn src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Adsel/simple-adventure.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Adsel/simple-adventure/issues"
  },
  "homepage": "https://github.com/Adsel/simple-adventure#readme",
  "devDependencies": {
    "@types/bcrypt": "^5.0.1",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/sockjs": "^0.3.33",
    "bcrypt": "^5.1.1",
    "express": "^4.19.2",
    "i": "^0.3.7",
    "jsonwebtoken": "^9.0.2",
    "mysql": "^2.18.1",
    "npm": "^10.8.1",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^4.6.1",
    "sockjs": "^0.3.24",
    "ts-node-dev": "^2.0.0",
    "typeorm": "^0.3.20",
    "typescript": "^5.4.5"
  },
  "keywords": []
}
